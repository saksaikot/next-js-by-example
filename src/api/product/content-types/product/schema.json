{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "cart_item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart-item.cart-item",
      "mappedBy": "product"
    }
  }
}
